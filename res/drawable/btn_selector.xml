<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
    <shape>
    <gradient android:startColor="#25B6FF" android:endColor="#61F7FD" android:angle="270"/>
    <corners android:radius="8dp"/>
    <stroke android:color="#eee"/>
    </shape>
    </item>
    <item android:state_pressed="false"><shape>
    <gradient android:startColor="#0000" android:endColor="#0000" />
    </shape></item>
</selector>
